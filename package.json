{
  "name": "battlebot",
  "main": "build/src/index.js",
  "types": "./typings/index.d.ts",
  "repository": "https://github.com/Archive-Discord/battle-bot-slash",
  "author": "kiss8981",
  "license": "GPL-3.0",
  "scripts": {
    "start": "yarn run build && cd build && node src/index.js",
    "start:pm2": "yarn run build && yarn run pm2:start",
    "dev": "ts-node-dev --transpile-only --respawn --trace-warnings --trace-uncaught -- ./src/index.ts",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "test": "yarn run lint:fix && yarn run format:fix && yarn run build",
    "pm2:start": "cd build && pm2 start ../process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:delete": "pm2 delete process.json"
  },
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/rest": "^1.0.0",
    "@discordjs/voice": "^0.11.0",
    "anyid": "^1.0.7",
    "archive-pubg-ts": "^1.0.0",
    "axios": "^0.27.2",
    "captcha-canvas": "^3.1.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "comma-number": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dayjs": "^1.11.4",
    "discord.js": "^14.0.3",
    "dokdo": "Archive-Discord/dokdo",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fs": "^0.0.1-security",
    "googleapis": "^105.0.0",
    "hcs.js": "^2.1.3",
    "korcen": "^0.2.0",
    "mongoose": "^6.4.7",
    "node-cache": "^5.1.2",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.7",
    "randomstring": "^1.2.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "string-progressbar": "^1.0.4",
    "strip-ansi": "^5.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/comma-number": "^2.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.4",
    "@types/randomstring": "^1.1.8",
    "@types/request-promise": "^4.1.48",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*",
    "build/"
  ],
  "packageManager": "yarn@3.1.1"
}
