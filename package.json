  "license": "MIT",
  "scripts": {
    "start": "yarn run build && cd build && node src/index.js",
    "start:pm2": "yarn run build && yarn run pm2:start",
    "dev": "ts-node-dev --transpile-only --respawn --trace-warnings --trace-uncaught -- ./src/index.ts",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "pm2:start": "pm2 start process.json",
    "pm2:stop": "pm2 stop process.json",
    "pm2:delete": "pm2 delete process.json"
  },
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.8.0",
    "@sapphire/ts-config": "^3.1.6",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.178",
    "@types/nodemailer": "^6.4.4",
    "@types/request": "^2.48.8",
    "anyid": "^1.0.7",
    "axios": "^0.25.0",
    "captcha-canvas": "^3.1.0",
    "chalk": "^3.0.0",
    "cheerio": "^1.0.0-rc.10",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord-api-types": "^0.26.1",
    "discord-player": "^5.2.2",
    "discord.js": "13.3.1",
    "dokdo": "^0.5.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "fs": "^0.0.1-security",
    "googleapis": "^95.0.0",
    "mongoose": "^6.1.2",
    "nodemailer": "^6.7.2",
    "opusscript": "^0.0.8",
    "randomstring": "^1.2.2",
    "regex-parser": "^2.2.11",
    "request": "^2.88.2",
    "socket.io": "^4.4.1",
    "string-progressbar": "^1.0.4",
    "strip-ansi": "^5.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "^16.11.12",
    "@types/randomstring": "^1.1.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*",
    "build/"
  ],
  "packageManager": "yarn@3.1.1"
}
